{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "$id": "http://neuralnine.com/my-person-schema",
  "title": "Person",
  "properties": {
    "name": {
      "type": "string"
    },
    "age": {
      "type": "integer",
      "minimum": 0
    },
    "isStudent": {
      "type": "boolean"
    },
    "favouriteProgrammingLanguage": {
      "type": "string"
    }
  },
  "required": [
    "name", "age"
  ],
  "additionalProperties": false
}